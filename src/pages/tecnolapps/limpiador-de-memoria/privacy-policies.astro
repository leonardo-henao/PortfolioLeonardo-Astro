<script is:inline>
  if (window == undefined || window == null) {
    window.location.href = '/tecnolapps/limpiador-de-memoria/en/';
  } else {
    const language = navigator.language;
    const path = window.location.pathname;

    if (!path.endsWith('/')) path = `${path}/`;

    if (language.includes('es') || language.includes('ES')) window.location.href = `${path}es/`;
    else window.location.href = `${path}en/`;
  }
</script>
